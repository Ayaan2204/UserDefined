#include <stdio.h>
void main()
{
    char a[10];
    char b[10];
    int x=0;
    int i=0;
    int diff;
    printf("Enter string 1\n");
    gets(a);
    printf("Enter string 2\n");
    gets(b);
   for(i=0;x==0;i++)
   {
       diff=a[i]-b[i];
       if(diff!=0)
       {
           x=1;
       }
       if(a[i]=='\0'||b[i]=='\0')
       {
           x=1;
       }
   }
   printf("%d",diff);
    
    
}
