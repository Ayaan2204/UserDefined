#include <stdio.h>

void main()
{
   char a[100];
   char b[100];
   gets(a);
   int i=0;
   int x=0;
   int j=0;
   int n;
   
   for(i=0;x==0;i++)
   {
       if(a[i]=='\0')
       {
           x=1;
       }
   }
   i=i-2;
   for(j=0;i>=0;i--)
   {
       b[j]=a[i];
       j++;
   }
   puts(b); 
}
