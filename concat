#include <stdio.h>
void main()
{
    char a[10];
    char b[10];
    int x=0;
    int i=0;
    int j=0;
    printf("Enter FIRSTNAME\n");
    gets(a);
    printf("Enter LASTNAME\n");
    gets(b);
    //Lenth of first
    for(i=0;x<2;i++)
    {
        if(a[i]=='\0')
        {
            x=1;
        }
        if(x==1)
        {
            
            a[i]=b[j];
            j++;
        }
        if(b[j]=='\0')
        {
            x=2;
        }
    }
    puts(a);
    
    
}
